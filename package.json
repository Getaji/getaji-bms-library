      "node-fetch@<2.6.7": ">=2.6.7",
      "cross-fetch@>=3.0.0 <3.1.5": ">=3.1.5",
      "ws@>=7.0.0 <7.4.6": ">=7.4.6",
      "dicer@<=0.3.1": ">=0.3.1",
      "jpeg-js@<0.4.4": ">=0.4.4",
      "got@<11.8.5": ">=11.8.5"
